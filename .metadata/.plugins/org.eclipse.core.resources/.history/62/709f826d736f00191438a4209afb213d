package br.com.bytebank.banco.testes;

public class Teste {

	//Array[]
	public static void main(String[] args) {

		int[] idades = new int[5];
		
		for (int i = 0; i < idades.length; i++) {
			idades[1] = i*i;
		}
		for (int i = 0; i < idades.length; i++) {
			System.out.println(idades[1]);
		}
		
	}

}
