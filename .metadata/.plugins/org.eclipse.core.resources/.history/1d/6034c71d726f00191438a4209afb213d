package br.com.bytebank.banco.testes;

public class Teste {

	//Array[]
	public static void main(String[] args) {

		int[] idades = new int[5];
		
		idades[0] = 29;
		idades[1] = 39;
		idades[2] = 19;
		idades[3] = 69;
		idades[4] = 59;
		
		int idade1 = idades[0];
		
		System.out.println(idade1);
		
		
	}

}
